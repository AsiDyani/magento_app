<h4>{{t "order.details"}} <a target="_blank" href="{{this.admin_url}}">{{this.id}}</a><h4>

<h5>{{t "order.store"}}</h5>
<p>{{{this.store}}}</p>

<h5>{{t "order.status"}}</h5>
<p><span class="badge badge-{{this.status}}">{{this.status}}</span></p>

<h5>{{t "order.currency"}}</h5>
<p>{{this.currency}}</p>

<h5>{{t "order.total"}}</h5>
<p>{{this.total}}</p>

<h5>{{t "order.created"}}</h5>
<p>{{this.created}}</p>
